package rohin.gameengine.test;

public class Application
{
    // ---------------------------------------------------------------------------------------------------------------------------------------------------------
    
    // ---------------------------------------------------------------------------------------------------------------------------------------------------------
    
    public Application ()
    {   
    }
    
    // ---------------------------------------------------------------------------------------------------------------------------------------------------------
    
    public void run()
    {
        CustomEngine testEngine = new CustomEngine ();
        
        testEngine.seta       ( 0 );
        testEngine.setb       ( 15 );
        testEngine.settrigger ( 6 );
        
        testEngine.run ();
    }
    
    // ---------------------------------------------------------------------------------------------------------------------------------------------------------
}
